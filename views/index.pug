  //  three Modal 
  .modal.fade#exampleModal1(tabindex="-1", aria-labelledby="exampleModalLabel", aria-hidden="true")
    .modal-dialog
      .modal-content.text-center
        .modal-header.text-center
          h5.modal-title.justify-content-center#exampleModalLabel Guests
          button.btn-close(type="button", data-bs-dismiss="modal", aria-label="Close")
        .modal-body
          .value-button.btn-outline-primary.text-center.dot#decrease(style="cursor: pointer;float: left;", onclick="decreaseValue()", value="Decrease Value") -
          input.btn-outline-primary.text-center#number(type="number", value="0")
          .value-button.btn-outline-primary.text-center.dot#increase(style="cursor: pointer; float: right;", onclick="increaseValue()", value="Increase Value") +
        .modal-footer
          button.btn.btn-secondary(type="button", data-bs-dismiss="modal") Apply
          //  <button type="button" class="btn btn-primary">Save changes</button> 
  .modal.fade#exampleModal2(tabindex="-1", aria-labelledby="exampleModalLabel", aria-hidden="true")
    .modal-dialog
      .modal-content
        .modal-body
          p
            label(for="amount") Price range:
            input#amount(type="text", readonly, style="border:0; color:#f6931f; font-weight:bold;")
          #slider-range
        .modal-footer
          button.btn.btn-secondary(type="button", data-bs-dismiss="modal") Close
  .modal.fade#exampleModal3(tabindex="-1", aria-labelledby="exampleModalLabel", aria-hidden="true")
    .modal-dialog
      .modal-content
        .modal-header
          h5.modal-title#exampleModalLabel Modal title
          button.btn-close(type="button", data-bs-dismiss="modal", aria-label="Close")
        .modal-body
          p#search
          p#search1
          p#search2
          p#search3
          p#search4
        .modal-footer
          button.btn.btn-secondary(type="button", data-bs-dismiss="modal") Close
